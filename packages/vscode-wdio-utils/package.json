{
    "name": "@vscode-wdio/utils",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.js"
        },
        "./node": {
            "import": "./dist/normalize.js"
        }
    },
    "scripts": {
        "build-production": "npm run clean-build",
        "build": "tsc -p .",
        "typecheck": "tsc --noEmit --project tsconfig.json",
        "clean-build": "npm run clean && npm run build",
        "clean": "shx rm -rf out dist coverage",
        "test": "vitest run .",
        "watch": "tsc -p . --watch"
    },
    "dependencies": {
        "@vscode-wdio/constants": "workspace:*",
        "@vscode-wdio/logger": "workspace:*"
    },
    "devDependencies": {
        "@vscode-wdio/types": "workspace:*"
    }
}
