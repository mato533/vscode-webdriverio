{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist"]
        },
        "typecheck": {
            "dependsOn": ["^build", "^typecheck"],
            "cache": false
        },
        "dev": {
            "persistent": true,
            "cache": false
        },
        "vscode-webdriverio#package": {
            "dependsOn": ["^build", "^typecheck"]
        }
    }
}
