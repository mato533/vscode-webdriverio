{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist"]
        },
        "build:production": {
            "dependsOn": ["^build:production"],
            "outputs": ["dist"],
            "cache": false
        },
        "typecheck": {
            "dependsOn": ["^build", "^typecheck"],
            "cache": false
        },
        "vscode-webdriverio#package": {
            "dependsOn": ["build:production"]
        }
    }
}
